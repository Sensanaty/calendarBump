{
  "name": "calendarBump",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "bundle": "npm run format:write && npm run build",
    "format:write": "prettier --write **/*.ts",
    "build:calendar": "ncc build .github/actions/calendarBump/index.ts -m -o .github/actions/calendarBump/dist",
    "build:changelog": "ncc build .github/actions/createChangelog/index.ts -m -o .github/actions/calendarBump/dist",
    "build:watch:calendar": "npm run build:calendar --watch",
    "build:watch:changelog": "npm run build:changelog --watch",
    "build": "npm run build:calendar && npm run build:changelog"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "devDependencies": {
    "@types/node": "^20.11.20",
    "@vercel/ncc": "^0.38.1",
    "prettier": "^3.2.5",
    "typescript": "^5.2.2",
    "vite": "^5.1.4"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0"
  }
}
